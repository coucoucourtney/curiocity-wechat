<!--地图容器-->
<map
id="myMap"
style="width: 100%; height: 80vh;"
longitude="{{longitude}}" latitude="{{latitude}}"
scale='17'
polyline="{{polyline}}"
markers="{{markers}}"
include-points="{{temp}}"
>
</map>
<view class="direction-box">
  <view class="h2">{{name}}</view>
  <view class="h3"> {{address}}</view>
  <view class="direction-info">
    <view class="p">Distance: {{walk.distance}}  meters </view>
    <view class="p">Duration: {{ walk.duration}}  min</view>
  </view>
</view>
