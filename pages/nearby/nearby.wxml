<!--longitude及latitude为设置为调转到指定地址位置，默认不显示-->
<map
id="myMap"
style="width: 100%; height: 100vh;"
longitude="{{longitude}}" latitude="{{latitude}}"
scale='17'
subkey="{{mapKey}}"
polyline="{{polyline}}"
markers="{{markers}}"
bindmarkertap="bindMarkertap"
></map>
<van-action-sheet show="{{ show }}" title="{{name}}" bind:close="onClose" >
  <view>description: {{description}}</view>
</van-action-sheet>